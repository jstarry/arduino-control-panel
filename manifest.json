{
  "manifest_version": 2,
  "name": "Arduino Control",
  "description": "Chrome App to control my Arduino",
  "version": "0.1.0",
  "app": {
    "background": {
      "scripts": ["chrome/background.js"]
    }
  },
  "icons": {
    "16": "chrome/icon-16x16.jpeg",
    "128": "chrome/icon-128x128.jpeg"
  },
  "permissions": [
    "serial"
  ]
}
